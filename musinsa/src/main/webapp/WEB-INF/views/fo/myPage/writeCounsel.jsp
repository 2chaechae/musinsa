function find_order(page) {

	var period = $('#f1 [name=period]').val();

	if($('#f1 [name=cmd]').val() == 'edit') {
		alert('주문번호는 수정이 불가합니다. 다른 주문번호로 문의를 원하시는 경우 신규 작성해주시기 바랍니다.');
		return false;
	}
	if(page != '0') {
		var f_ord_no = $("#f1 [name='ord_no']").val();

		$.ajax({
			type: "GET",
			url: "/app/util/find_order",
			data: "page=" + page + "&period="+ period +"&ord_no=" + f_ord_no,
			success: function(msg) {
				$("#orderListPopup").html(msg);
				uiLayerBasic('#orderListPopup');
			}
		});
        $("body").removeClass('fixedScroll');
	} else {
		$("#orderListPopup").removeClass('is-active');
		$(".ui-layer-close.fixed").css('display','none');
		$("body").removeClass('fixedScroll');
	}
}

function setOrder() {
	var order_info = $('input:radio[name="order_list"]:checked').val();

	if(order_info == undefined) {
		alert("주문을 선택해 주세요.");
		return false;
	}

	var a_order_info = order_info.split("|");
	var ord_no = (a_order_info[0] != undefined) ? a_order_info[0] : "";
	var ord_opt_no = (a_order_info[1] != undefined) ? a_order_info[1] : "";
	var goods_no = (a_order_info[2] != undefined) ? a_order_info[2] : "";

	if(ord_no != "" && goods_no != '') {
			$.ajax({
				type: "POST"
				, url: "/app/cs/get_order_info/"+ord_no+"/"+goods_no
				, success: function(msg){
					eval("var json = " + msg);
					$('#f1 [name=ord_no]').val(json.ord_no);
					$('#f1 [name=goods_no]').val(json.list[0].goods_no);
					var product_html =  "<div class='n-prd-row'><span class='img-block'><a href='/app/goods/"+json.list[0].goods_no+"/"+json.list[0].goods_sub+"'><img src='//image.msscdn.net"+parseGoodsImageUrlBySize(json.list[0].img, 100)+"' alt='"+json.list[0].goods_nm+"'></a></span>";
					product_html += "<ul class='info'><li class='brand'><a href='https://www.musinsa.com/brands/"+json.list[0].brand+"'>"+json.list[0].brand_nm+"</a></li><li class='name'><a href='/app/goods/"+json.list[0].goods_no+"/"+json.list[0].goods_sub+"'>"+json.list[0].goods_nm+"</a></li></ul></div>";
					$("#product_info").html(product_html);
					$("#orderListPopup").removeClass('is-active');
					$(".ui-layer-close.fixed").css('display','none');
					$("body").removeClass('fixedScroll');
				}
			});

		// document.location.href = "/app/cs/counsel?ord_no=" + ord_no + "&goods_no="+ goods_no;
	}
}

function uploadImage() {
	$("#file_message").text('').hide();
	var ff = $('#upload_form').get(0);
	var formData = new FormData(ff);

	$.ajax({
		cache : false,
		url : "/xf/lib/fileupload.php?MODE=m",
		processData: false,
		contentType: false,
		type : 'POST',
		data : formData,
		success : function(response) {
			try {
				var callback = response;
				if (matches = response.replace(/(?:\r\n|\r|\n)/g, '').match(/\<script\>(.*)\<\/script\>/)) {
					callback = $.trim(matches[1]);
				}
				eval(callback);
			} catch (e) {
			}
		}
	});

	ff.reset();
}

function SetImage(result, message, file_url) {
	var ff = document.upload_form;

	if(result) {
		var image_tag = "<li><img src=\"" + file_url + "\" /><a class=\"del-image\" href=\"javascript:void(0);\"><i class=\"ic-14-line-close ic-white\"></i></a></li>";
		ff.files.value += (ff.files.value != "") ? "," + file_url : file_url;
		$("#file_show").append(image_tag);
	} else {
		$("#file_message").text(message).show();
	}
}
function selectParentType(el) {
	var parentNo = el.value || 0;
	if (!parentNo) {
		alert('문의 유형을 선택해 주세요.');
		return;
	}
	getChildQnaTypeList(parentNo);
}
var qnaTypeAjaxXhr = null;
function getChildQnaTypeList(parentNo) {
	if (!parentNo) {
		return;
	}
	if (qnaTypeAjaxXhr) {
		qnaTypeAjaxXhr.abort();
	}
	qnaTypeAjaxXhr = $.ajax({
		type: "GET",
		url: "/app/cs/getQnaTypeListByParentNo/" + parentNo,
		dataType: "json",
		success: function (res) {
			if (res.cd != 200) {
				alert('문의 유형을 불러오는데 실패했습니다. 다시 시도해 주세요.');
				return;
			}
			try {
				var qnaTypeList = res.data.qna_type_list || [],
						tpl = '<option value="">문의 유형을 선택해주세요.</option>';
				qnaTypeList.forEach(function (item) {
					tpl += '<option value="' + item.id + '">' + item.val + '</option>';
				});
				$('#qna-type-select').html(tpl);
			} catch (e) {
				alert('문의 유형을 불러오는데 실패했습니다. 다시 시도해 주세요.');
			}
		},
		error: function () {
			if (qnaTypeAjaxXhr.statusText === 'abort') {
				return;
			}
			alert('문의 유형을 불러오는데 실패했습니다. 다시 시도해 주세요.');
		}
	});
}

