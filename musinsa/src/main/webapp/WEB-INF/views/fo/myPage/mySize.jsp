	// document ready
	$(document).ready(function(){
		$(".form").hide();
		$("#mysize_default").show();
		typeBtnList('all');
		typeBtnSet();
		typeBtnAct('default');

		// category click
		$('.mysize-category .sort').on('click',function(){
			var $this = $(this),
					category = $this.data('category');
			if(!$this.hasClass('is-active')){
				$('.mysize-category .sort').removeClass('is-active');
				$this.addClass('is-active');
				typeBtnList(category);
			}
			typeBtnSet();
			var img = $('#typeBtn .sort').first().data('img');
			var s_no = $('#typeBtn .sort').first().attr('s_no');
			// Form 변경
			$(".form").hide();
			$("#mysize_" + s_no).show()
			$("input[name='s_no']").val(s_no);

			typeImage(img);
			typeBtnAct();
		});

		// type arrow click
		var is_animating = false;
		$('.mysize-type .arrow').on('click',function() {
			var btnW = $('#typeBtn .sort.is-active').innerWidth(),
					offset = $('#typeBtn').scrollLeft();
			if($(this).hasClass('prev')) {
				if(!is_animating) {
					is_animating = true;
					$('#typeBtn').animate({scrollLeft: offset - btnW}, 250, function () {
						is_animating = false;
					});
				}
			} else {
				if(!is_animating) {
					is_animating = true;
					$('#typeBtn').animate({scrollLeft: offset + btnW}, 250, function () {
						is_animating = false;
					});
				}
			}
		});

		// 첫 로드시 해당 화면으로 이동
		var s_no = $("input[name='s_no']").val();
		if(s_no != ""){
			$("div").find("button[s_no='"+s_no+"']").trigger("click");
			$("div").find("button[s_no='"+s_no+"']").focus();
		} else {
			$("div").find("button[s_no='default']").trigger("click");
		}

		//피부고민 3개 초과 선택 시도 시
		$("input:checkbox[name='skinWorry']").on('change', function (e) {
			if ($("input:checkbox[name='skinWorry']:checked").length > 3) {
				$(this).prop('checked', false);
				alert("피부고민은 최대 3개까지 선택할 수 있습니다");
			}
		});
	});

	// window resize
	$(window).resize(function(){
		// RE-setting
		setTimeout(function(){
			$('#typeBtn').scrollLeft(0);
			typeBtnSet();
		},500);
	});

	// type btn
	function typeBtnList(type){
		if(type === 'all') var types = sizeTypes;
		else var types = sizeTypes.filter(function(button){ return button.category == type });

		var typeBtn = '';
		for(var i = 0; i < types.length; i++){
			typeBtn += '<button type="button" class="sort '+ types[i].category +'" data-img="'+ types[i].icon +'"s_no="' + types[i].s_no +'">'
					+ '<i class="my-ico '+ types[i].class +'"></i>' + types[i].name
					+ '</button>';
		}
		$('#typeBtn').html(typeBtn).promise().done(function() {
			$('#typeBtn').find('button.sort').eq(0).addClass('is-active');
		});

		var s_no = $('#typeBtn').find('.sort').eq(0).attr("s_no");

		// 최근 구매 내역 실측 입력 여부 판단
		//select_recently_order(s_no);

	}

	// type setting
	function typeBtnSet(){
		var wrapOffset = $('#typeBtn').offset().left;
		$('#typeBtn .sort').each(function(){
			var offset = $(this).offset().left;
			$(this).attr('data-offset',offset - wrapOffset);
		});
	}

	// type click
	function typeBtnAct(){
		$('#typeBtn .sort').on('click', function(){
			var $this = $(this),
					img = $this.data('img'),
					offset = $this.data('offset'),
					wrap = $('#typeBtn');
			if (!$this.hasClass('is-active')) {
				$('#typeBtn .sort').removeClass('is-active');
				$this.addClass('is-active');
				wrap.animate({scrollLeft: offset}, 250);
			}

			// s_no Setting
			var s_no = $this.attr("s_no");
			$("input[name='s_no']").val(s_no);

			// Form 변경
			$(".form").hide();
			$("#mysize_" + s_no).show()

			// 이미지 변경
			typeImage(img);
		});
	}

	// 구매 내역 보기
	function saerch_order_list(size_type, page) {
		var div_id = "pop_order_size";

		$.ajax({
			type: "GET",
			url: "/app/mypage/size_order_list/"+size_type + "/" + page,
			success: function(msg) {
				if(msg == "-1") {
					alert("로그인 후 사용할 수 있습니다.");
				} else if(msg == "-2") {
					alert("사이즈 타입이 공백이거나 숫자가 아닙니다.");
				} else {
					$("#" + div_id).html(msg);

					var top = Math.max(0, (($(window).height() - $("#" + div_id).outerHeight()) / 2) + $(window).scrollTop()) + "px";
					$("#" + div_id).css("top", top);

					$("#" + div_id).show();

					$("#" + div_id).jqm();
					$("#" + div_id).jqmShow();
				}
			}
		});
	}

	function activeTooltib(id) {
		if($("#" + id).hasClass('active')){
			$("#" + id).removeClass("active");
		}else{
			$("#" + id).addClass("active");
		}
	}

	function showAction(showId,hideId) {
		$("#" + showId + "Tab").show();
		$("#" + showId + "Title").addClass("is-active");
		$("#" + hideId + "Tab").hide();
		$("#" + hideId + "Title").removeClass("is-active");
	}

	function toggleAction(id){
		if(id == "mySkin"){
			showAction("mySkin","mySize");
		}else{
			showAction("mySize","mySkin");
		}
		$("#tab_id").val(id);
	}
	toggleAction($("#tab_id").val());

	function orderSizeHide() {
		var div_id = "pop_order_size";
		$("#" + div_id).hide();
		$("#" + div_id).jqmHide();
	}

	// input img
	function typeImage(img){
		$('#mySizeImg').attr('src', img);
	}
</script>

