<script type="text/javascript">
	var size_cnt = "0";
	var order_history_cnt = "0";
	
	// 최근 구매 내역 선택
	function select_recently_order(type_no) {
		if(type_no != "") {
			$.ajax({
				type: "POST",
				url: "/app/api/mysize/get_size_order_history",
				data: {"type_no" : type_no},
				success: function (msg) {
					eval("var json = " + msg);
					var size_cnt = json.size_cnt;
					var order_history_cnt = json.order_history_cnt;

					if(type_no != "" && size_cnt == "0" && order_history_cnt != "0") {
						if(confirm("최근 구매 내역 중 실측을 고르시겠습니까?")) {
							//확인
							$.ajax({
								type: "GET",
								url: "/app/mypage/set_lately_mysize",
								data: {type_no : type_no},
								success: function(msg) {
									if(msg == "1") {
										alert("최근 상품의 사이즈가 정상적으로 저장되었습니다.");
										document.location.href = "/app/mypage/mysize_new/" + type_no;
									} else if(msg == "-1") {
										alert("최근 구매 상품이 존재하지 않습니다.");
									} else if (msg == "-2") {
										alert("옵션이 존재하지 않습니다.");
									} else if (msg == "-3") {
										alert("최근 구매 상품의 실측 정보가 존재하지 않습니다.");
									} else {
										alert("사이즈 저장 시 문제가 발생했습니다.\n다시 시도해 주시기 바랍니다.");
										document.location.reload();
									}
								}
							});
						}
					}
				}
			});
		}
	}

	// size input 클릭시 최근 구매 컨펌창 오픈
	function select_recently(size_cnt, type_no) {

		var empty_value = 0;
		for(i=1; i<=size_cnt; i++){
			var size_value = $("#mysize_"+type_no+" input[name='size_val']").val();

			if(size_value == ""){
				empty_value++;
			}
		}

		if(empty_value == size_cnt){
			select_recently_order(type_no);
		}
	}

	// 폼 초기화 -> 저장한 마이사이즈 삭제로 기능변경
	function resetSize() {
		if (!confirm('등록하신 사이즈 정보를 삭제하시겠습니까?')) {
			return false;
		}

		var s_no = $("input[name='s_no']").val();
		var mysize_info = $("#mysize_" + s_no)[0];
		if (s_no == 'default') {
			mysize_info.height.value = '';
			mysize_info.weight.value = '';
		} else {
			for (var i = 0; i < mysize_info.size_val.length; i++) {
				mysize_info.size_val[i].value = '';
			}
		}

		if (mysize_info.has_saved_data.value == 'Y') {
			deleteSize(s_no);
		}
	}

	function deleteSize(s_no) {
		$.ajax({
			type: "GET",
			url: "/app/mypage/delete_size",
			data: {
				s_no: s_no
			},
			success: function(res) {
				var response = JSON.parse(res);
				if(response.cd != 1) {
					alert(response.msg);
				}
				setTimeout(function() {
					document.location.href = "/app/mypage/mysize_new/" + s_no;
				}, 500);
			}
		});
	}

	// 사이즈 저장
	function saveSize() {
		if (inSaveSizeAjax) {
			alert('사이즈 저장 중입니다. 잠시만 기다려 주세요.');
			return;
		}

		if(!$('#mysizeAgree').is(':checked')) {
			alert('사이즈 정보수집 및 이용동의를 체크해주세요.');
			$('#mysizeAgree').focus();
			return false;
		}

		var s_no = $("input[name='s_no']").val();
		var ff = document.getElementById("mysize_"+s_no);
		var type_no = s_no;
		var pattern = /^[-]?\d+(?:[.]\d+)?$/;
		var size_info = "";		// 타입별 사이즈 정보(타입번호|아이템|사이즈)

		if(type_no != "default") {
			if(ff.size_val.length) {
				for(var i=0; i<ff.size_val.length; i++) {
					if(ff.size_val[i].value != "") {
						if(!pattern.test(ff.size_val[i].value)) {
							alert("사이즈는 숫자 및 소수점까지 입력할 수 있습니다.\n다시 입력해 주세요");
							ff.size_val[i].focus();
							return false;
						}

						// 최대 최소 범위가 있을 경우에는 체크
						if(ff.minRange[i].value != "" ||  ff.maxRange[i].value != "") {
							var size_val = parseInt(ff.size_val[i].value);
							var min_range = parseInt(ff.minRange[i].value);
							var max_range = parseInt(ff.maxRange[i].value);

							if(size_val < min_range || size_val > max_range) {
								var alert_html = ff.sizeName[i].value + "은(는) ";

								// 최소범위가 있는경우
								if (ff.minRange[i].value != "") {
									alert_html += "최소 " + ff.minRange[i].value;
									alert_html += "부터 ";
								}
								// 최대범위가 있는경우
								if (ff.maxRange[i].value != "") {
									alert_html += "최대 " + ff.maxRange[i].value;
									alert_html += "까지 ";
								}
								alert_html += "입력 할 수 있습니다.";

								ff.size_val[i].focus();
								alert(alert_html);
								return false;
							}
						}

						size_info += type_no + "|" + ff.sizeName[i].value + "|" + ff.size_val[i].value + "\n";
					} else {
						alert("실측 사이즈를 입력해주세요.");
						return false;
					}
				}
			} else {
				if(ff.size_val.value != "") {
					if(!pattern.test(ff.size_val.value)) {
						alert("사이즈는 숫자 및 소수점까지 입력할 수 있습니다.\n다시 입력해 주세요");
						ff.size_val.focus();
						return false;
					}
					size_info = type_no + "|" + ff.sizeName.value + "|" + ff.size_val.value + "\n";
				}
			}
			ff.size_info.value = size_info;
		} else {
			// 신체 치수인 경우
			var height = ff.height.value;
			var weight = ff.weight.value;

			if(height == "") {
				alert("키를 입력해주세요.");
				return false;
			} else {
                height = parseInt(height);
				if(height < 100 || height > 220){
					alert("키는 최소 100부터 220까지 입력할 수 있습니다.\n다시 입력해 주세요");
					ff.height.focus();
					return false;
				}
			}

			if(weight == "") {
				alert("몸무게를 입력해주세요.");
				return false;
			} else {
                weight = parseInt(weight);
				if(weight < 30 || weight > 150){
					alert("몸무게는 최소 30부터 150까지 입력할 수 있습니다.\n다시 입력해 주세요");
					ff.weight.focus();
					return false;
				}
			}
		}

		inSaveSizeAjax = true;

		$.ajax({
			type: "GET",
			url: "/app/mypage/save_size",
			data: $("#mysize_"+s_no).serialize(),
			success: function(msg) {
				if(msg == "1") {
					alert("사이즈가 정상적으로 저장되었습니다.");
				} else if(msg == "-1") {
					alert("로그인 후 사용할 수 있습니다.");
				} else {
					alert("사이즈 저장 시 문제가 발생했습니다.\n관리자에게 문의해 주시기 바랍니다.");
				}
				//document.location.href = "/app/mypage/mysize_new/" + s_no;
			}, error : function(request, status, error) {
				alert("사이즈 저장 시 문제가 발생했습니다.\n관리자에게 문의해 주시기 바랍니다.");
			}, complete: function() {
				inSaveSizeAjax = false;
			}
		});
	}

	function apply_size_order_list(size_type) {
		var obj = $("input[name=sel_product]");
		var idx = "";
		var contents = "";
		var mypage_yn = $("input[name=mypage_yn]").val();

		for(var i=0; i<obj.length; i++) {
			if(obj.eq(i).is(":checked")) {
				idx = i;
				break;
			}
		}

		if(idx != "0" && idx == "") {
			alert("적용 할 상품을 선택해 주세요");
			return false;
		}

		if(mypage_yn == "Y") {
			for(var j=0; j< $("input[name=size_"+ idx + "]").length; j++) {
				var size = $("input[name=size_"+ idx + "]").eq(j).val();
				size = (size == "") ? 0 : $("input[name=size_"+ idx + "]").eq(j).val();
				$("form[id='mysize_" + size_type + "'] input[name=size_val]").eq(j).val(parseInt(size));
			}
		} else {
			contents = "<th>MY</th>";
			for(var i=0; i < $("input[name=size_"+ idx + "]").length; i++) {
				contents += "<td class=\"mysize_val\">" + $("input[name=size_"+ idx + "]").eq(i).val() + "</td>"
			}

			$("#mysize").html(contents).promise().done(function() {
				$("#save_mysize").show();
			});
		}

		var div_id = "pop_order_size";
		$("#" + div_id).hide();
		$("#" + div_id).jqmHide();
	}

	function skinValidate() {
		//skinType, skinTone, skinWorry
		if($("input:radio[name='skinType']:checked").length < 1){
			alert("피부타입을 선택해주세요.");
			return false;
		}

		if($("input:checkbox[name='skinWorry']:checked").length > 3){
			alert("피부고민은 최대 3개까지 선택할 수 있습니다.");
			return false;
		}

		return true;
	}

	function saveSkin() {

		if(!$('#myskinAgree').is(':checked')) {
			alert('피부 정보수집 및 이용동의를 체크해주세요.');
			$('#myskinAgree').focus();
			return false;
		}

		if (!this.skinValidate()) {
			return false;
		}

		var skinWorrys='';
		$("input:checkbox[name='skinWorry']:checked").each(function(){
			skinWorrys += (skinWorrys == "")? $(this).val():","+$(this).val();
		});

		var params = $("#skinType01, #skinTone01").serialize();
		params += "&skinWorrys=" + skinWorrys;

		$.ajax({
			type: "GET",
			url: "/app/mypage/save_skin",
			data: params,
			success: function(msg) {
				if(msg == "1") {
					alert("피부정보가 정상적으로 저장되었습니다.");
				} else if(msg == "-1") {
					alert("로그인 후 사용할 수 있습니다.");
				} else {
					alert("저장 시 문제가 발생했습니다.\n관리자에게 문의해 주시기 바랍니다.");
				}
			}, error : function(request, status, error) {
				alert("저장 시 문제가 발생했습니다.\n관리자에게 문의해 주시기 바랍니다.");
			}, complete: function() {

			}
		});
	}

	function resetSkin() {
		if (!confirm('등록하신 피부 정보를 삭제하시겠습니까?')) {
			return false;
		}
		$.ajax({
			type: "GET",
			url: "/app/mypage/delete_skin",
			data: {
			},
			success: function(res) {
				var response = JSON.parse(res);
				if(response.cd != 1) {
					alert(response.msg);
				}
				setTimeout(function() {
					//document.location.href = "/app/mypage/mysize_new?tab_id=mySkin";
				}, 500);
			}
		});
	}

</script>

