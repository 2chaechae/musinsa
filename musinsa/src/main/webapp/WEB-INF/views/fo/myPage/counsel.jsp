	// 리스트 Toggle
	$(document).ready(function(){
		$('.my-question').on('click',function(e) {
			var $this = $(this);
			var $wrap = $this.parents('tr');
			var list_cnt = $wrap.attr("list_cnt");
			var $cont = $wrap.next('.list_cnt_' + list_cnt);
			var $answer = $cont.nextAll('.list_cnt_' + list_cnt);

			if($cont.hasClass('is-active')) {
				$cont.removeClass('is-active');
				if($answer.length) $answer.removeClass('is-active');
			} else {
				$cont.addClass('is-active');
				if($answer.length) $answer.addClass('is-active');
			}
		});

		$('.btn-del').on('click', function() {
			var qna_board_no =  $(this).data('qna_board_no');
			if(confirm('정말 삭제하시겠습니까?')) {

				var dataList = {
					'cmd' : 'del',
					'qna_board_no' : qna_board_no,
					'return_type' : 'json'
				};

				$.ajax({
					type: "post",
					url: "/app/mypage/counsel_save",
					data: dataList,
					success: function(json){
						var result = $.parseJSON(json);

						if(result.cd == '1') {
							alert('정상적으로 삭제되었습니다.');
							window.location.reload();
						} else {
							if(result.msg) {
								alert(result.msg);
								window.location.reload();
							} else {
								alert('삭제에 실패했습니다. 다시 시도해주세요.');
							}
						}
					}
				});
			}
		});
	});

	/* 20191213 UI-HS 팝업창 가운데로 띄우기 */
	function reportReviewsPop(url){

		if(!loginChk()) {
			var msg = confirm("신고는 로그인 후 작성하실 수 있습니다.\n로그인하시겠습니까?");
			if (msg == true){
