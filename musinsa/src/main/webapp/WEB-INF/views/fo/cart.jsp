}

function addCart() {
	var ff = document.prinfo;
	var obj = ff.rel_prd;
	var is_last = "N";
	var goods = new Array();

	var recommend_goods = getCheckValue(obj);

	if(recommend_goods.length == 0) {
		alert("원하는 상품을 한 개 이상 체크 하셔야 합니다.");
		return false;
	}

	if(!relValidate(recommend_goods)) {
		return false;
	}

	// 장바구니 등록
	for(var i=0; i<recommend_goods.length; i++) {
		goods = recommend_goods[i].split("-");
		var goods_no = goods[0];
		var goods_sub = goods[1];
		var opt_cnt = goods[2];
		var first_option_cnt = goods[3];

		if(i == recommend_goods.length-1) {
			is_last = "Y";
		} else {
			is_last = "N";
		}

		relativeOrder.addCart(opt_cnt, goods_no, goods_sub, first_option_cnt, is_last, 'N');
	}
}

function getCheckValue(obj) {
	var ret = new Array();
	if(obj) {
		if(obj.length) {
			for( var i=0; i<obj.length; i++) {
				if(obj[i].checked) {
					ret.push(obj[i].value);
				}
			}
		} else {
			if(obj.checked) {
				ret.push(obj.value);
			}
		}
	}
	return ret;
}

function relValidate(recommend_goods) {
	// 옵션체크
	if(!relativeOrder.checkOption(recommend_goods)) {
		alert('옵션을 선택하세요');
		return false;
	} else if (!relativeOrder.checkAddOption(recommend_goods)) {
		alert('추가 옵션을 선택하세요');
		return false;
	} else if (!relativeOrder.checkJaego(recommend_goods)) {
		alert('재고가 부족합니다.');
		return false;
	}
	return true;
}

