$(document).ready(function () {

	// 전체선택 체크박스의 초기 체크/미체크 설정
	var cart_all_cnt = $("input[name=cart_no]").length;
	var cart_checked_cnt = $("input[name=cart_no]:checked").length;
	var cart_sold_out_cnt = $('.disabled_cart').length;
	if (cart_all_cnt != cart_sold_out_cnt) {
		if (cart_all_cnt == cart_checked_cnt) {
			$(".chk_all").prop('checked', true);
			$(".chk_all").prop('disabled', false);
		}
		if (cart_checked_cnt > 0) {
			$("#del_chk").removeClass("click_disalbe");
		}
		$("#del_chk").show();
	} else {
		// 품절상품만 존재할 경우 (체크박스가 한개도 없을 경우)
		$('#chk_all').prop('disabled', true);
		$(".chk_all").prop('checked', false);
		$("#del_chk").hide();
	}

	$(".chk_all").click(function(){
		var chk = $(this).is(":checked");
		if(chk){
			$(".checked_cart").prop("checked", true);
			// 선택 삭제 활성화
			if($("#del_chk").hasClass("click_disalbe")){
				$("#del_chk").removeClass("click_disalbe");
			}
		}else{
			$(".checked_cart").prop("checked", false);
			// 선택삭제 비활성화
			if(!$("#del_chk").hasClass("click_disalbe")){
				$("#del_chk").addClass("click_disalbe");
