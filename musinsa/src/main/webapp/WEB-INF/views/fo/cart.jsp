// 전체 구매시 구매 못하는 상품 제거
function sel_goods_order() {

	var cart_goods = $("input[name='cart_no'][type='checkbox']");
	var sale_yn = $("input[name='sale_yn'][type='hidden']");
	var sales_pause_yn = $("input[name='sales_pause_yn'][type='hidden']");
	var domestic_disable_order_yn = $("input[name='domestic_disable_order_yn'][type='hidden']");
	var good_qty = $("input[name='good_qty'][type='hidden']");

	var used_yn = $("input[name='used_yn'][type='hidden']");
	var cart_qty = $("input[name='cart_qty'][type='text']");

	var offline_goods_yn = $("input[name='offline_goods_yn'][type='hidden']");
	var goods_nms = $("input[name='goods_nm'][type='hidden']");
	var brand_nms = $("input[name='brand_nm'][type='hidden']");

	var raffle_yn = $("input[name='raffle_yn'][type='hidden']");
	var raffle_purchase = $("input[name='raffle_purchase'][type='hidden']");

	var app_goods_yn = $("input[name='app_goods_yn'][type='hidden']");

	var release_yn = $("input[name='release_yn'][type='hidden']");
	var release_dt = $("input[name='release_dt'][type='hidden']");

	var total_cnt = 0;
	var able_to_by_cnt = 0;
	var cannot_to_by_cnt = 0;
	var app_goods_by_cnt = 0;
	var release_goods_by_cnt = 0;
	var release_goods_nm = "";
	var release_brand_nm = "";
	var goods_nm = "";

	for(var i=0; i<cart_goods.length; i++) {
		if(cart_goods[i].checked){
			var now = new Date();

			var year= now.getFullYear();
			var mon = (now.getMonth() + 1) > 9 ? ''+(now.getMonth() + 1) : '0'+(now.getMonth() + 1);
			var day = now.getDate() > 9 ? '' + now.getDate() : '0' + now.getDate();
			var hour = now.getHours() > 9 ? '' + now.getHours() : '0' + now.getHours();
			var minute = now.getMinutes() > 9 ? '' + now.getMinutes() : '0' + now.getMinutes();
			var now_ymdt = (year+mon+day+hour+minute);

			if((sale_yn[i].value == 'N' || good_qty[i].value == '0') || sales_pause_yn[i].value == "Y" || domestic_disable_order_yn[i].value == "Y" || ((used_yn[i].value == "E" || used_yn[i].value == "F") && cart_qty[i].value > 1) || offline_goods_yn[i].value == "Y" || (raffle_yn[i].value == "Y" && raffle_purchase[i].value == "N") || (release_yn[i].value == "Y" && release_dt[i].value > now_ymdt) || app_goods_yn[i].value == "Y"){
				if(goods_nm == "") {
					goods_nm = goods_nms[i].value;
				}
				if(app_goods_yn[i].value == "Y") {
					app_goods_by_cnt++;
				}
				if (release_yn[i].value == "Y" && release_dt[i].value > now_ymdt) {
					release_goods_by_cnt++;
					if (release_goods_nm == "" && release_brand_nm == "") {
						release_goods_nm = goods_nms[i].value;
						release_brand_nm = brand_nms[i].value;
					}
				}
				cannot_to_by_cnt++;
			} else {
				able_to_by_cnt++;
			}
			total_cnt++;
		}
	}

	if(cannot_to_by_cnt > 0) {
		// 앱전용상품이 하나라도 존재할 경우 주문 불가
		if( app_goods_by_cnt > 0 ){
			alert("무신사 앱에서 구매 가능한 상품이 선택되어 있습니다. 앱에서 이용해주십시오.");
			return false;
		}

		if (release_goods_by_cnt > 0) {
			var msg = '[' + release_brand_nm + '] ' + release_goods_nm;
			if (release_goods_by_cnt == 1) {
				msg += ' 상품은 판매 예정으로 구매가 불가능합니다.';
				if (cannot_to_by_cnt != total_cnt) {
					msg += ' 판매 예정인 상품을 제외하고 구매해주세요.';
				}
			} else {
				msg += ' 외 ' + (release_goods_by_cnt-1) + '개의 상품은 판매 예정으로 구매가 불가능합니다. 판매 예정인 상품을 제외하고 구매해주세요.';
			}
			alert(msg);
			return false;
		}

		if(cannot_to_by_cnt == total_cnt) {
			for(var i=0; i<cart_goods.length; i++) {
				if(cart_goods[i].checked){
					if(sale_yn[i].value == 'N' || good_qty[i].value == '0') {
						alert("품절된 상품은 구매 할 수 없습니다.");
						return false;
					} else if(sales_pause_yn[i].value == "Y" || domestic_disable_order_yn[i].value == "Y") {
						alert("판매 중지 상품은 구매할 수 없습니다.");
						return false;
					} else if((used_yn[i].value == "E" || used_yn[i].value == "F") && cart_qty[i].value > 1) {
						alert("구매수량이 제한된 상품입니다.\n수량을 다시 선택해 주세요.");
						return false;
					} else if(offline_goods_yn[i].value == "Y") {
						alert("오프라인 판매 전용 상품은 테라스 매장에서 구매할 수 있습니다.");
						return false;
					} else if(raffle_yn[i].value == "Y" && raffle_purchase[i].value == "N"){
						alert("래플 구매 기간이 아닙니다.");
						return false;
					}
				}
			}
		} else {
			if(cannot_to_by_cnt == 1) {
				// 하나만 못구매하는경우
				if(confirm("선택하신 " + goods_nm + " 은 품절 등의 사유로 주문이 불가합니다.\n주문불가 상품을 제외하고 " + able_to_by_cnt + " 개의 상품을 주문하시겠습니까?")) {
					for(var i=0; i<cart_goods.length; i++) {
						if(cart_goods[i].checked){
							if((sale_yn[i].value == 'N' || good_qty[i].value == '0') || sales_pause_yn[i].value == "Y" || domestic_disable_order_yn[i].value == "Y" || ((used_yn[i].value == "E" || used_yn[i].value == "F") && cart_qty[i].value > 1) || offline_goods_yn[i].value == "Y" || (raffle_yn[i].value == "Y" && raffle_purchase[i].value == "N")){
								// 선택 해제
								cart_goods[i].checked = false;
							}
						}
					}
					Cart.selectOrder();
				}
			} else {
				// 기타등등
				if(confirm("선택하신 " + goods_nm + " 외 " + (cannot_to_by_cnt-1) + " 개의 상품은 품절 등의 사유로 주문이 불가합니다.\n주문불가 상품을 제외하고 " + able_to_by_cnt + " 개의 상품을 주문하시겠습니까?")) {
					for(var i=0; i<cart_goods.length; i++) {
						if(cart_goods[i].checked){
							if((sale_yn[i].value == 'N' || good_qty[i].value == '0') || sales_pause_yn[i].value == "Y" || domestic_disable_order_yn[i].value == "Y" || ((used_yn[i].value == "E" || used_yn[i].value == "F") && cart_qty[i].value > 1) || offline_goods_yn[i].value == "Y" || (raffle_yn[i].value == "Y" && raffle_purchase[i].value == "N")){
								// 선택 해제
								cart_goods[i].checked = false;
							}
						}
					}
					Cart.selectOrder();
				}
			}
		}
	} else {
		Cart.selectOrder();
	}

}

