	});

	$(".checked_cart").click(function() {
		var chk_cnt = $("input[name=cart_no]:checked").length;
		var all_cnt = $("input[name=cart_no]").length;

		// 선택삭제 버튼에 대한 활성화/비활성화 설정
		if(chk_cnt == 0){
			// 선택삭제 비활성화
			if(!$("#del_chk").hasClass("click_disalbe")){
				$("#del_chk").addClass("click_disalbe");
			}
		} else {
			// 선택 삭제 활성화
			if($("#del_chk").hasClass("click_disalbe")){
				$("#del_chk").removeClass("click_disalbe");
			}
			if (chk_cnt == all_cnt) {
				$(".chk_all").prop('checked', true);
			} else {
				$(".chk_all").prop('checked', false);
			}
		}

		// 전체선택 체크박스 체크/미체크 설정
		if($("input[name=chk_all]").prop("checked") == true){
			if ($("input[name=cart_no]").length == chk_cnt) {
				$("input[name=chk_all]").prop("checked", true);
			} else {
				$("input[name=chk_all]").prop("checked", false);
			}
