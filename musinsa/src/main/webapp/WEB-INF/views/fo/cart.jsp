		var obj = this;
		setTimeout(function () {
			calGiftSuggestion(obj);
			calTotalAmt();
			if (plcc_open_yn == 'Y') {
				PLCC.checkPLCCFirstPayDiscountAmt();
			}
			Cart.checkExclusiveMsspay();
		},100);
	});

	$("#hyundaicardFirstPayment").click(function() {
		if ($('#hyundaicardFirstPayment').hasClass('is-active')) {
