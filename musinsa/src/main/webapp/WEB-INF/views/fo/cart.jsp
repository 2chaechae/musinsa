function calTotalAmt() {
	var totalNormalPrice = 0;
	var totalNormalAddOptPrice = 0; // 정상가 + 옵션가 + 추가옵션가
	var totalPrdPrice = 0;
	var totalDlvAmt = 0;
	var totalSaleAmt = 0;
	var totalSaleAddOptAmt = 0; // 세일가 + 옵션가 + 추가옵션가
	var totalDcAmt = 0;
	var totalPtnDcAmt = 0;
	var totalPrePointAmt = 0;
	var totalPointAmt = 0;
	var totalCouponAmt = 0;
	var maxUsePoint = 0;	// 보유 적립금 가능 금액 계산
	var usePointYn = '';
	var totalPayDcAmt = 0;
	var totalPayDcAddOptAmt = 0; // 결제할인가 + 옵션가 + 추가옵션가
	var totalUsePoint = 0;

	var useGroupDiscountLimitYn = '';
	var totalGroupDiscountAmt = 0;	// 스냅그룹할인 금액 계산
	var groupDiscountRatio = $("input[name='group_dc_ratio']").val();
	var groupDiscountRemainAmt = $("input[name='group_dc_remain_amt']").val();

	$('.cart_table .checked_cart:checked').each(function (index, obj) {
		var wrapper = $(obj).closest('tr');

		totalPrePointAmt += +(wrapper.find("input[name='prepoint_amt']").val() || 0);
		totalPointAmt += +(wrapper.find("input[name='point_amt']").val() || 0);
		totalDlvAmt += +(wrapper.find("input[name='dlv_amt']").val() || 0);
		totalDcAmt += +(wrapper.find("input[name='dc_amt']").val() || 0);
		totalPtnDcAmt += +(wrapper.find("input[name='ptn_dc_amt']").val() || 0);

		var normalPrice = +(wrapper.find("input[name='goods_normal_price']").val() || 0);
		var goodsPrice = +(wrapper.find("input[name='good_price']").val() || 0);
		var goodsOptPrice = +(wrapper.find("input[name='goods_opt_price']").val() || 0);
		var goodsAddOptPrice = +(wrapper.find("input[name='goods_addopt_amt']").val() || 0);
		var cartQty = +(wrapper.find("input[name='cart_qty']").val() || 0);
		usePointYn = wrapper.find("input[name='use_point_yn']").val();
		totalNormalPrice += normalPrice * cartQty;
		totalNormalAddOptPrice += normalPrice * cartQty + goodsOptPrice + goodsAddOptPrice;
		totalPrdPrice += goodsPrice * cartQty;
		totalSaleAmt += (normalPrice - goodsPrice) * cartQty;
		totalSaleAddOptAmt += (normalPrice - goodsPrice) * cartQty + goodsOptPrice + goodsAddOptPrice;
		useGroupDiscountLimitYn = wrapper.find("input[name='group_dc_limit_yn']").val();

		if (usePointYn != 'Y') {
			maxUsePoint += ceil((goodsPrice * 0.07), 1) * cartQty;
		}

		if (useGroupDiscountLimitYn == 'N') {
			totalGroupDiscountAmt += ceil((goodsPrice * (groupDiscountRatio/100)), 1) * cartQty;
		}

		var no = +(wrapper.find("input[name='no']").val() || 0);
		if (appliedCoupon.length > 0) {
			appliedCoupon.forEach(function (couponData) {
				if (couponData.length >= 6 && couponData[2] == no) {
					totalCouponAmt += +(couponData[5]);
				}
			});
		}
	});

	totalUsePoint = parseInt($('[name=point]').val());

	totalPayDcAmt = parseInt(totalSaleAmt + totalUsePoint + totalDcAmt + totalPtnDcAmt);
	totalPayDcAmt += totalPrePointAmt;

	totalPayDcAddOptAmt = parseInt(totalSaleAddOptAmt + totalUsePoint + totalDcAmt + totalPtnDcAmt);
	totalPayDcAddOptAmt += totalPrePointAmt;

	$('#max_use_point').text(Comma(maxUsePoint));
	var pointInput = $('#pointGuide input[name=point]');
	if (unComma(pointInput.val()) > maxUsePoint) {
		pointInput.val(Comma(maxUsePoint));
	}

	$('.cart_normal_price').val(totalNormalPrice);
	$('.cart_normal_add_opt_price').text(Comma(totalNormalAddOptPrice));
	$('.cart_total_save').val(totalPayDcAmt);
	$('.cart_total_add_opt_save').text(Comma(totalPayDcAddOptAmt));

	$('#cart_total_sale_amt').text(Comma(totalSaleAddOptAmt));
	$('#total_prd_amt').text(Comma(totalPrdPrice));
	$('#total_dlv_amt').text(Comma(totalDlvAmt));
	// $('#sale_amt').text(Comma(totalSaleAmt));
	// $('#dc_price').text(Comma(totalDcAmt));
	$('#cart_user_dc_amt').text(Comma(totalDcAmt));
	// $('#ptn_dc_amt').text(Comma(totalPtnDcAmt));
	$('#cart_ptn_dc_amt').text(Comma(totalPtnDcAmt));
	$('#cart_prepoint_amt').text(Comma(totalPrePointAmt));

	// $('[name=total_normal_price]').val(totalNormalPrice);
