}

function setCouponData() {
	var coupon_apply_yn = $('#coupon_apply_yn').val();

	if(coupon_apply_yn == 'y') {
		var coupon_data = getBrowserData('coupon');

		var a_coupon_use_data = [];
		var sum_dc_amt = 0;

		if(coupon_data && coupon_data != '') {
			var coupon_json = JSON.parse(coupon_data);

			for(var i=0; i<coupon_json.length; i++) {
				var goods_coupon_info = coupon_json[i].goods_coupon_info;
				var goods_cnt = coupon_json[i].goods_cnt != "undefined" ? coupon_json[i].goods_cnt : 1;
				var exist_goods_check = $("input[id='goods_"+coupon_json[i].goods_no + "_" + coupon_json[i].goods_sub + "_" + coupon_json[i].opt+"']").val();

				if(typeof exist_goods_check != "undefined") {
					if (goods_coupon_info && goods_coupon_info != "") {
						a_coupon_use_data.push(goods_coupon_info);
						var a_goods_coupon_info = goods_coupon_info.split("|");
						var coupon_amt = $("#cart_qty_"+ a_goods_coupon_info[2]).val() * (a_goods_coupon_info[5]/goods_cnt);

						a_goods_coupon_info[5] = coupon_amt;
						coupon_json[i].goods_coupon_info = a_goods_coupon_info.join('|');
						sum_dc_amt += parseInt(coupon_amt, 10);
					}
				}
			}

			if(coupon_json) {
				setBrowserData('coupon', JSON.stringify(coupon_json));
			}

			var coupon_use_data = '';
			if (a_coupon_use_data) {
				coupon_use_data = a_coupon_use_data.join("\t");
			}

			CheckPayCoupon(sum_dc_amt, coupon_use_data);
			calTotalAmt();
		}
	} else if(getBrowserData('coupon')) {
		setBrowserData('coupon', '');
	}
}

var appliedCoupon = [];
function CheckPayCoupon(totalCouponDCAmt, couponData) {
	var ff = document.f1;
	ff.kcoupon.value = totalCouponDCAmt;

	var totalPayAmt = Cart.getPayAmt();
	if (totalPayAmt < 1000) {
		alert('최소 결제 금액은 1,000 원 입니다');
		ff.kcoupon.value = 0;
		if(getBrowserData('coupon')) {
			setBrowserData('coupon', '');
		}
		return false;
	}

	appliedCoupon = couponData.split('\t').map(function (couponData) {
		return couponData.split('|');
	});

	return true;
}

