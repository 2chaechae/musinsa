    function getBrandRanking(type) {

        if($.trim($("#brand_rank_"+type).html()) == "") {
            var eventRankType = type;
            var goodsImageWidth = 320;
            var api_url = search_api + "/api/ranking/v2/event/brand?eventCode=" + html_code + "&goodsImageWidth=" + goodsImageWidth
					+ "&period=" + type + "&offset=0&sex=" + THIS_PAGE_GF + "&siteKindId=musinsa&size=10&kids=" + is_kids;

            $.ajax({
                type: 'get',
                async: false,
                url: api_url,
                success: function (_msg) {

                    var brandDataList = _msg.data.brandRankList;
                    var brandDataTime = _msg.data.brandRankUpdateTime;
					var innerHtml = "";

					if(brandDataList.length > 0) {
						var timeText = brandDataTime.substring(0,4)+"."+brandDataTime.substring(5,7)+"."+brandDataTime.substring(8,10)+" "+brandDataTime.substring(11,16);

						innerHtml += '<ol class="CBrandRanking__list" id="CBrandRankingList">';
						for(var i = 0; i < brandDataList.length; i++) {
							innerHtml += '<li class="CBrandRanking__item';
							if(i >= 10) {
								innerHtml += ' hidden-list';
							} else if(i == 0) {
								innerHtml += ' active';
							}
							innerHtml += '">';
							innerHtml += '<div class="CBrandRanking__item__information">';
							innerHtml += '<button type="button" class="CBrandRanking__item__button" data-index="'+ brandDataList[i].rank +'">';
							innerHtml += '<span class="CBrandRanking__item__rank">'+ brandDataList[i].rank +'</span>';
							if(brandDataList[i].variation > 0) {
								innerHtml += '<span class="CBrandRanking__item__rank-indication CBrandRanking--increase">상승 </span>';
							} else if(brandDataList[i].variation < 0) {
								innerHtml += '<span class="CBrandRanking__item__rank-indication CBrandRanking--decrease">하락 </span>';
							} else {
								innerHtml += '<span class="CBrandRanking__item__rank-indication">고정 </span>';
							}
							innerHtml += '<span class="CBrandRanking__item__brand-name">'+ brandDataList[i].brandName +'</span>';
							innerHtml += '<span class="CBrandRanking__item__brand-link">';
							innerHtml += '<svg width="30px" height="30px" viewBox="0 0 30 30" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="svg-icon-arrow">';
							innerHtml += '<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">';
							innerHtml += '<polygon fill="#000000" points="12.8 7.189 20.607 14.996 12.8 22.802 13.479 23.481 21.964 14.996 13.479 6.51" class="svg-color"></polygon>';
							innerHtml += '</g>';
							innerHtml += '</svg>';
							innerHtml += '</span>';
							innerHtml += '</button>';
							innerHtml += '</div>';
							innerHtml += '<div class="CBrandRanking__item__goods">';
							innerHtml += '<div class="list-container column6">';
							if(brandDataList[i].goodsList != null) {
								for(var j = 0; j < brandDataList[i].goodsList.length; j++) {
									if(j < 6) {
										innerHtml += '<div class="list-item">';
										innerHtml += '<div class="CGoods">';
										innerHtml += '<div class="CGoods__thumbnail">';
										innerHtml += '<em class="CGoods__ranking">'+ (j+1) + '</em>';

										innerHtml += '<a href="/app/goods/'+ brandDataList[i].goodsList[j].goodsNo +'" class="CGoods__thumbnail__img">';
										innerHtml += '<img src="'+ brandDataList[i].goodsList[j].imageUrl +'" alt="">';
										innerHtml += '</a>';
										innerHtml += '</div>';
										innerHtml += '<a href="/app/goods/'+ brandDataList[i].goodsList[j].goodsNo +'" class="CGoods__information">';
										innerHtml += '<span class="CGoods__name">'+ brandDataList[i].goodsList[j].goodsName +'</span>';
										innerHtml += '<div class="CGoods__price">';
										innerHtml += '<span class="CGoods__price__org">'+ Comma(brandDataList[i].goodsList[j].price) +'</span>';
										if(brandDataList[i].goodsList[j].saleRate > 0){
											innerHtml += '<span class="CGoods__price__rate">'+ brandDataList[i].goodsList[j].saleRate +'</span>';
											innerHtml += '<del class="CGoods__price__normal">'+Comma(brandDataList[i].goodsList[j].normalPrice)+'</del>';
										}
										innerHtml += '</div>';
										innerHtml += '</a>';
										innerHtml += '</div>';
										innerHtml += '</div>';
									}
								}
							}
							innerHtml += '</div>';
							innerHtml += '<div class="CButtons">';
							innerHtml += '<a href="https://www.musinsa.com/brands/'+ brandDataList[i].brand +'/goods" class="CButton CButton--radius CButton--link">'+ brandDataList[i].brandName +' 전체 상품 <svg width="30px" height="30px" viewBox="0 0 30 30" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="">';
							innerHtml += '<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">';
							innerHtml += '<polygon points="11.21 7.82 18.389 15 11.21 22.18 12.515 23.485 21 15 12.515 6.515" class="svg-color"></polygon>';
							innerHtml += '</g>';
							innerHtml += '</svg>';
							innerHtml += '</a>';
							innerHtml += '</div>';

							innerHtml += '</div>';
							innerHtml += '</li>';
						}
						innerHtml += '</ol>';
						innerHtml += '<p class="refresh-comment space-padding">'+timeText+':00 최근 1시간 판매 기준입니다.</p>';
						if(brandDataList.length > 10) {
							innerHtml += '<div class="CButtons">';
							innerHtml += '<a href="javascript:void(0);" class="CButton CButton--radius CButton--more CButton--more-brand" data-more-number="10" data-more-target="#" onclick="campaign.moreEventHandler(event);"> 더 보기 </a>';
							innerHtml += '</div>';
						}
						innerHtml += '</div>';


						$("#brand_rank_"+type).html(innerHtml);
						campaign.brandRankingEvent(document.getElementById("CBrandRankingList"));
					} else {
						$(".brand_rank_area").hide();
					}
                }

            });
        }
    }
    function getGoodsRanking(type, area) {

        var area_name = "#goods_rank_now_"+area;
        var mainCategoryList = "";

		if(area !== "all") {
			mainCategoryList = area;
		}

        if($.trim($(area_name).html()) == "") {
            var goodsImageWidth = 320;
            var api_url = search_api + "/api/ranking/v2/event/goods?eventCode=" + html_code + "&goodsImageWidth=" + goodsImageWidth
					+ "&mainCategoryList=" + mainCategoryList +  "&period=" + type + "&offset=0&sex=" + THIS_PAGE_GF + "&siteKindId=musinsa&size=60&kids=" + is_kids;

            $.ajax({
                type: 'get',
                async: false,
                url: api_url,
                success: function (_msg) {

                    var goodsDataList = _msg.data.goodsRankList;
                    var goodsDataTime = _msg.data.goodsRankUpdateDate;
                    var innerHtml = "";

					if(goodsDataList.length > 0) {
						var timeText = goodsDataTime.substring(0,4)+"."+goodsDataTime.substring(5,7)+"."+goodsDataTime.substring(8,10)+" "+goodsDataTime.substring(11,16);

						innerHtml += '<div class="list-container column6">';
						for(var i = 0; i < goodsDataList.length; i++) {
							var variationClass = "";
							if(goodsDataList[i].variation > 0){
								variationClass = "up";
							} else if(goodsDataList[i].variation < 0) {
								variationClass = "down";

							}
							innerHtml += '<div class="list-item';
							if(i >= 12) innerHtml += ' hidden-list';
							innerHtml += '">';
							innerHtml += '<div class="CGoods">';
							innerHtml += '<div class="CGoods__thumbnail">';
							innerHtml += '<em class="CGoods__ranking">'+goodsDataList[i].rank+'</em>';
							innerHtml += '<a href="/app/goods/'+ goodsDataList[i].no +'" class="CGoods__thumbnail__img">';
							innerHtml += '<img src="'+goodsDataList[i].imageUrl+'" alt="">';
							innerHtml += '</a>';
							innerHtml += '</div>';
							innerHtml += '<a href="/app/goods/'+ goodsDataList[i].no +'" class="CGoods__information">';
							innerHtml += '<span class="CGoods__brand">'+goodsDataList[i].brandName+'</span>';
							innerHtml += '<span class="CGoods__name">'+goodsDataList[i].name+'</span>';
							innerHtml += '<div class="CGoods__price">';
							innerHtml += '<span class="CGoods__price__org">'+Comma(goodsDataList[i].price)+'</span>';
							if(goodsDataList[i].saleRate > 0)
							{
								innerHtml += '<span class="CGoods__price__rate">'+goodsDataList[i].saleRate+'</span>';
								innerHtml += '<del class="CGoods__price__normal">'+Comma(goodsDataList[i].normalPrice)+'</del>';
							}
							innerHtml += '</div>';
							innerHtml += '</a>';
							innerHtml += '</div>';
							innerHtml += '</div>';
						}
						innerHtml += '</div>';
						if (goodsDataList.length === 0) {
							innerHtml += '<div class="n-content-none"><p>등록된 상품이 없습니다.<br>선택한 필터를 변경해보세요.</p></div>';
						} else {
							innerHtml += '<p class="refresh-comment space-padding">'+timeText+':00 최근 1시간 판매 기준입니다.</p>';
						}
						if(goodsDataList.length > 12){
							innerHtml += '<div class="CButtons">';
							innerHtml += '<a href="javascript:void(0);" class="CButton CButton--radius CButton--more CButton--more-brand" data-more-number="12" data-more-target="#" onclick="campaign.moreEventHandler(event);">더 보기 </a>';

							innerHtml += '<a href="' + search_web + '/ranking/best?period=now&age=' + THIS_PAGE_GF + '&mainCategory=' + mainCategoryList + '&subCategory=&leafCategory=&price=&golf=false&newProduct=false&exclusive=false&discount=false&soldOut=false&page=1&viewType=small&device=&priceMin=&priceMax=" class="CButton CButton--radius CButton--link">랭킹 더보기<svg width="18px" height="18px" viewBox="0 0 18 18" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="svg-icon-arrow">';
							innerHtml += '<title>@@icon_text</title>';
							innerHtml += '<g stroke="none" stroke-width="1" fill-rule="evenodd">';
							innerHtml += '<polygon fill-rule="nonzero" transform="translate(5.989592, 9.000000) scale(1, -1) rotate(-135.000000) translate(-5.989592, -9.000000) " points="2.73959236 12.25 2.73959236 4.75 1.73959236 4.75 1.73959236 13.25 10.2395924 13.25 10.2395924 12.25" class="svg-color"></polygon>';
							innerHtml += '</g>';
							innerHtml += '</svg>';
							innerHtml += '</a>';

							innerHtml += '</div>';
						}
						innerHtml += '</div>';
					}

                    $(area_name).html(innerHtml);
                }

            });
        }

    }

    function Comma(numstr) {
        var numstr = String(numstr);
        var re0 = /(\d+)(\d{3})($|\..*)/;
        if (re0.test(numstr))
            return numstr.replace(re0, function(str,p1,p2,p3) { return Comma(p1) + "," + p2 + p3; });
        else
            return numstr;
    }

    $(document).ready(function() {
        if(rank_yn) {
            if($(".brand_rank_area").length > 0) getBrandRanking("now");
            if($(".goods_rank_area").length > 0) getGoodsRanking("now", "all");
        }

        var nodes = $(".CTab__list");
        nodes.each(function(){
            if($(this).children(".CTab__button").length < 2) $(this).hide();
        });

    });

	$("img.lazyload").lazyload({
		threshold : 200
	});

	function getTabData(tab_uid)
	{
		if($("#tab_"+tab_uid).html().replace(/\n|\r|\s*/g, "") == "") {
			$.ajax({
				type: "POST",
				url: "/app/campaign/get_campaign_tab_data",
				data : {
					'tab_uid' : tab_uid,
					'gf_use' : gf_use,
					'now_date' : now_date
				},
				success: function(msg) {
					eval("var json = " + msg);
					if (json != '') {
						$("#tab_"+tab_uid).html(json);
					}
					$("img.lazyload").lazyload({});
				}
			});
		}
	}

</script>



</body></html>