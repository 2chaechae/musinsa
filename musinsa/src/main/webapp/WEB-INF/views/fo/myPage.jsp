		/**
		 * fire claim Event after checking order current state
		 * @param {element} element
		 * @param {string} cmd
		 */
		function claimHandler(element, cmd) {
			var $element = element && $(element).parents('.btn-set');
			var ordOptNo = $element.data('ord-opt-no') || '';
			var ordState = $element.data('ord-state') || '';
			var ordNo = $element.data('ord-no') || '';
			var brand = $element.data('brand') || '';
			var goods_nm = $element.data('goods-nm') || '';
			var return_yn = $element.data('return-yn') || 'Y';
			cmd = cmd || '';

			if (!(ordNo && ordOptNo && ordState && cmd)) {
				return false;
			}

            var is_refundable = true;
			if ((cmd == 'cancelRefund' || cmd == 'refund' || cmd == 'offlineRefund') && $.inArray(brand, ['musinsastandard', 'musinsastandardkids']) > -1) {
				$.ajax({
					type: "POST",
					url: ["/app/api/mypage/check_refundable?ord_no=", ordNo].join(''),
					async: false,
					dataType: 'json',
					success: function (json) {
						is_refundable = json.data && json.data.check_refund_value;
					}
				});

				if (!is_refundable) {
					alert('힛탠다드 100원 이벤트 상품이 포함된 주문의 무신사 스탠다드 상품은 출고상태가 모두 동일할 경우에만 환불요청 가능합니다. 고객센터로 문의 부탁드립니다.');
					return false;
				}
			}

			$.ajax({
				type: "POST",
				url: ["/app/api/order/get_state?ord_opt_no=", ordOptNo].join(''),
				async: false,
				dataType:'json',
				success: function(response) {
					var currentOrdState = response.data.ord_state || null;
					if (ordState == currentOrdState) {
						switch (cmd) {
							case 'cancel':
								location.href=['/app/mypage/order_cancel', ordNo, ordOptNo, '?cmd=cancel'].join('/');
								break;
							case 'refund':
								if (return_yn == 'N') {
									alert("["+ goods_nm +"] 상품의 교환/환불 신청은 1:1문의 접수를 통해 가능합니다 \n접수완료 이후 빠른 시일 내에 고객센터에서 연락 드리겠습니다.");
									location.href=['/app/cs/counsel?', $.param({
										ord_no: ordNo,
										ord_opt_no: ordOptNo,
										goods_no: $element.data('goods-no'),
										faq_view_yn: 'Y',
										qna_type_no: 24
									})].join('');
									break;
								}
								location.href=['/app/mypage/order_refund', ordNo, ordOptNo].join('/');
								break;
							case 'cancelRefund':
								location.href=['/app/mypage/order_cancel', ordNo, ordOptNo, '?cmd=refund'].join('/');
								break;
							case 'digitalProduct':
								location.href=['/app/mypage/order_cancel', ordNo, ordOptNo, '?cmd=digital_product'].join('/');
								break;
							case 'offlineRefund':
								if (return_yn == 'N') {
									alert("["+ goods_nm +"] 상품의 교환/환불 신청은 1:1문의 접수를 통해 가능합니다 \n접수완료 이후 빠른 시일 내에 고객센터에서 연락 드리겠습니다.");
									location.href=['/app/cs/counsel?', $.param({
										ord_no: ordNo,
										ord_opt_no: ordOptNo,
										goods_no: $element.data('goods-no'),
										faq_view_yn: 'Y',
										qna_type_no: 24
									})].join('');
									break;
								}
								location.href=['/app/mypage/order_cancel', ordNo, ordOptNo, '?cmd=offline_refund'].join('/');
								break;
							case 'change':
								var goodsNo = $element.data('goods-no');
								var goodsSub = $element.data('goods-sub');
								showOperatorLayer(ordNo, ordOptNo, goodsNo, goodsSub);
								break;
							case 'releaseChange':
								if (return_yn == 'N') {
									alert("["+ goods_nm +"] 상품의 교환/환불 신청은 1:1문의 접수를 통해 가능합니다 \n접수완료 이후 빠른 시일 내에 고객센터에서 연락 드리겠습니다.");
									location.href=['/app/cs/counsel?', $.param({
										ord_no: ordNo,
										ord_opt_no: ordOptNo,
										goods_no: $element.data('goods-no'),
										faq_view_yn: 'Y',
										qna_type_no: 23
									})].join('');
									break;
								}
								location.href=['/app/mypage/order_exchange', ordNo, ordOptNo].join('/');
								break;
							case 'counsel':
								location.href=['/app/cs/counsel?', $.param({
									ord_no: ordNo,
									ord_opt_no: ordOptNo,
									goods_no: $element.data('goods-no'),
									faq_view_yn: 'Y'
								})].join('');
								break;
							case 'requestOrderCancel':
								var url = ["/app/mypage/order_cancel/", ordNo, ordOptNo].join('/');
								location.href=[url + '?', $.param({
									cmd: 'order_cancel_from_prepare_order_shipping'
								})].join('');
								break;
						}
					} else {
						alert("주문 상태가 변경되었습니다.\n주문 상태를 확인하여 주시기 바랍니다.");
						location.reload();
					}
				}
			});
		}

